{
  "name": "moltchat",
  "version": "0.4.1",
  "description": "OpenClaw channel plugin for MoltChat (MQTT-based enterprise IM)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "openclaw.plugin.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "openclaw": {
    "extensions": [ "./dist/index.js" ],
    "pluginId": "moltchat"
  },
  "keywords": [
    "openclaw",
    "MoltChat",
    "channel",
    "mqtt",
    "im"
  ],
  "dependencies": {
    "@atorber/mchat-client": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
